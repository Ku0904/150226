    {
        "id": "9c05286ac5264f3d",
        "type": "function",
        "z": "0e0ff0ab2da54f27",
        "name": "Format readings",
        "func": "const payload = msg.payload || {};\nconst now = new Date();\nconst timestamp = now.toLocaleString('en-GB', { hour12: false });\nconst reading = {\n    voltage: Number(payload.Voltage || 0).toFixed(2),\n    current: Number(payload.Current || 0).toFixed(2),\n    powerFactor: Number(payload.PowerFactor || 0).toFixed(2),\n    frequency: Number(payload.Frequency || 0).toFixed(2),\n    time: timestamp\n};\n\nlet rows = flow.get('logRows') || [];\nrows.unshift({\n    time: reading.time,\n    voltage: reading.voltage,\n    current: reading.current,\n    powerFactor: reading.powerFactor,\n    frequency: reading.frequency\n});\nrows = rows.slice(0, 400);\nflow.set('logRows', rows);\n\nreturn [\n    { payload: Number(reading.voltage), topic: 'Voltage', time: reading.time },\n    { payload: Number(reading.current), topic: 'Current', time: reading.time },\n    { payload: Number(reading.powerFactor), topic: 'Power Factor', time: reading.time },\n    { payload: Number(reading.frequency), topic: 'Frequency', time: reading.time },\n    { payload: rows, action: 'replace' }\n];",
        "outputs": 5,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
