    {
        "id": "6a231ddb3f6c48a3",
        "type": "function",
        "z": "0e0ff0ab2da54f27",
        "name": "Export log",
        "func": "const rows = flow.get('logRows') || [];\nif (!rows.length) { msg.ui_notification = {type:'warning', message:'No rows to export'}; return msg; }\nconst header = ['Time','Voltage','Current','PowerFactor','Frequency'];\nconst body = rows.map(r => [r.time,r.voltage,r.current,r.powerFactor,r.frequency].join(','));\nmsg.payload = [header.join(','), ...body].join('\n');\nmsg.filename = '/tmp/energy-data-log.csv';\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
