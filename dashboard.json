    {
        "id": "6f1abbc7c1454207",
        "type": "function",
        "z": "0e0ff0ab2da54f27",
        "name": "Chart zoom control",
        "func": "const key = msg.topic || 'chart';\nconst dir = msg.payload;\nconst stateKey = 'zoom-' + key;\nconst state = flow.get(stateKey) || { start: 0, end: 100 };\nconst step = 10;\nif (dir === 'in') {\n    const span = state.end - state.start;\n    if (span > 20) {\n        state.start += step/2;\n        state.end -= step/2;\n    }\n} else if (dir === 'out') {\n    state.start = Math.max(0, state.start - step/2);\n    state.end = Math.min(100, state.end + step/2);\n}\nflow.set(stateKey, state);\nmsg.ui_update = {\n    chartOptions: {\n        dataZoom: [{ type: 'slider', start: state.start, end: state.end, realtime: true, showDetail: true, bottom: 10 }]\n    }\n};\nreturn msg;",
        "outputs": 1,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
