    {
        "id": "3f764f08e0c7a1d0",
        "type": "function",
        "z": "8ffaea47a084d313",
        "name": "function 1",
        "func": "let voltage = +msg.payload.Voltage.toFixed(2);\nlet current = +msg.payload.Current.toFixed(2);\nlet pf = +msg.payload.PowerFactor.toFixed(2);\nlet freq = +msg.payload.Frequency.toFixed(2);\n\nreturn [\n    { payload: voltage },\n    { payload: current },\n    { payload: pf },\n    { payload: freq }\n];\n",
